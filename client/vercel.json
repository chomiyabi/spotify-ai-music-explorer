{
  "buildCommand": "npm run build:prod",
  "outputDirectory": "build", 
  "framework": "create-react-app",
  "installCommand": "npm ci",
  "env": {
    "REACT_APP_API_URL": "@react_app_api_url"
  },
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}